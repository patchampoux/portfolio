RewriteEngine On
RewriteBase /
RewriteCond %{HTTP_HOST} ^www\.(.*)$ [NC]
RewriteRule ^/(.*)$ http://%1/$1 [R=301,L]

#ErrorDocument 404 /patchampoux/portfolio/web/404.html
ErrorDocument 404 /404.html


#RewriteRule ^(.*/)?index\.php /patchampoux/portfolio/web/?section=accueil$1 [R=301,L]
RewriteRule ^(.*/)?index\.php /?section=accueil$1 [R=301,L]